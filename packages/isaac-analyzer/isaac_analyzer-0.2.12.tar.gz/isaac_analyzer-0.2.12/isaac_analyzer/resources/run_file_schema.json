{
    "$schema": "http://json-schema.org/draft-06/schema#",
    "$ref": "#/definitions/isaacRunFile",
    "definitions": {
        "isaacRunFile": {
            "type": "object",
            "additionalProperties": false,
            "properties": {
                "runs": {
                    "type": "array",
                    "items": {
                        "$ref": "#/definitions/run"
                    }
                }
            },
            "required": [
                "runs"
            ],
            "title": "isaacRunFile"
        },
        "run": {
            "type": "object",
            "additionalProperties": false,
            "properties": {
                "type": {
                    "enum": [
                        "hard",
                        "normal",
                        "greed",
                        "greedier"
                    ]
                },
                "seed": {
                    "type": "string"
                },
                "character": {
                    "type": "string"
                },
                "startingItems": {
                    "type": "array",
                    "items": {
                        "$ref": "#/definitions/item"
                    }
                },
                "ending": {
                    "type": "string",
                    "pattern": "^Ending [0-9]{1,2}$"
                },
                "floors": {
                    "type": "array",
                    "items": {
                        "$ref": "#/definitions/floor"
                    }
                }
            },
            "required": [
                "character",
                "ending",
                "floors",
                "seed",
                "type"
            ],
            "title": "run"
        },
        "floor": {
            "type": "object",
            "additionalProperties": false,
            "properties": {
                "number": {
                    "type": "integer"
                },
                "type": {
                    "enum": [
                        "Basement",
                        "Cellar",
                        "Burning Basement",
                        "Downpour",
                        "Dross",
                        "Caves",
                        "Catacombs",
                        "Flooded Caves",
                        "Mines",
                        "Ashpit",
                        "Depths",
                        "Necropolis",
                        "Dank Depths",
                        "Mausoleum",
                        "Gehenna",
                        "Womb",
                        "Utero",
                        "Scarred Womb",
                        "???",
                        "Corpse",
                        "Cathedral",
                        "Sheol",
                        "Chest",
                        "Dark Room",
                        "The Void",
                        "Home"
                    ]
                },
                "curse": {
                    "$ref": "#/definitions/curse"
                },
                "itemRoom": {
                    "$ref": "#/definitions/itemRoom"
                },
                "shop": {
                    "$ref": "#/definitions/shop"
                },
                "bossRoom": {
                    "$ref": "#/definitions/bossRoom"
                },
                "bossRoomXL": {
                    "$ref": "#/definitions/bossRoom"
                },
                "itemRoomXL": {
                    "$ref": "#/definitions/itemRoom"
                },
                "specialRooms": {
                    "type": "array",
                    "items": {
                        "$ref": "#/definitions/specialRoom"
                    }
                },
                "deal": {
                    "$ref": "#/definitions/deal"
                },
                "items": {
                    "type": "array",
                    "items": {
                        "$ref": "#/definitions/item"
                    }
                }
            },
            "required": [
                "bossRoom",
                "curse",
                "number",
                "type"
            ],
            "title": "floor"
        },
        "bossRoom": {
            "type": "object",
            "additionalProperties": false,
            "properties": {
                "visited": {
                    "type": "boolean"
                },
                "boss": {
                    "enum": [
                        "Monstro",
                        "None",
                        "Gemini",
