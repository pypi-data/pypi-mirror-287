                },
                "champion": {
                    "$ref": "#/definitions/champion"
                },
                "items": {
                    "type": "array",
                    "items": {
                        "$ref": "#/definitions/item"
                    }
                }
            },
            "required": [
                "visited"
            ],
            "title": "bossRoom"
        },
        "item": {
            "type": "object",
            "additionalProperties": false,
            "properties": {
                "id": {
                    "anyOf": [
                        {
                            "type": "string"
                        },
                        {
                            "type": "number"
                        }
                    ]
                },
                "taken": {
                    "type": "boolean"
                }
            },
            "required": [
                "id",
                "taken"
            ],
            "title": "item"
        },
        "itemRoom": {
            "type": "object",
            "additionalProperties": false,
            "properties": {
                "rerollMachine": {
                    "type": "boolean"
                },
                "visited": {
                    "type": "boolean"
                },
                "items": {
                    "type": "array",
                    "items": {
                        "$ref": "#/definitions/item"
                    }
                }
            },
            "required": [
                "visited"
            ],
            "title": "itemRoom"
        },
        "shop": {
            "type": "object",
            "additionalProperties": false,
            "properties": {
                "rerollMachine": {
                    "type": "boolean"
                },
                "visited": {
                    "type": "boolean"
                },
                "boss": {
                    "enum": [
                        "Greed",
                        "Super Greed"
                    ]
                },
                "items": {
                    "type": "array",
                    "items": {
                        "$ref": "#/definitions/item"
                    }
                }
            },
            "required": [
                "visited"
            ],
            "title": "shop"
        },
        "specialRoom": {
            "type": "object",
            "additionalProperties": false,
            "properties": {
