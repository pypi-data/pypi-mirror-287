              <apply>
                <times/>
                <ci> parameter1 </ci>
                <ci> species5 </ci>
              </apply>
            </apply>
          </math>
        </kineticLaw>
      </reaction>
      <reaction id="reaction6" name="Reaction 6">
        <listOfReactants>
          <speciesReference species="species6"/>
        </listOfReactants>
        <listOfProducts>
          <speciesReference species="species7"/>
        </listOfProducts>
        <kineticLaw substanceUnits="substance">
          <math xmlns="http://www.w3.org/1998/Math/MathML">
