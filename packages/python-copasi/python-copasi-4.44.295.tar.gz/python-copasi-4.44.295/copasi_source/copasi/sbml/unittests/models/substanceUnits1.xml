              <apply>
                <times/>
                <ci> parameter1 </ci>
                <ci> species3 </ci>
              </apply>
            </apply>
          </math>
        </kineticLaw>
      </reaction>
      <reaction id="reaction4" name="Reaction 4">
        <listOfReactants>
          <speciesReference species="species4"/>
        </listOfReactants>
        <listOfProducts>
          <speciesReference species="species5"/>
        </listOfProducts>
        <kineticLaw substanceUnits="substance">
          <math xmlns="http://www.w3.org/1998/Math/MathML">
