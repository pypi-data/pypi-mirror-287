<odoo>
<data>
<record id="picking_category_eqi" model="picking_type_category">
    <field name="name">Equipment In</field>
    <field name="code">EQI</field>
    <field name="direction">incoming</field>
    <field name="default_source_location_type_id" ref="location_type_equipment" />
    <field
                name="default_destination_location_type_id"
                ref="ssi_stock.location_type_main_stock"
            />
    <field
                name="allowed_source_location_type_ids"
                eval="[(6, 0, [ref('location_type_equipment')])]"
            />
    <field
                name="allowed_destination_location_type_ids"
                eval="[(6, 0, [ref('ssi_stock.location_type_main_stock')])]"
            />
</record>

<record id="picking_category_eqo" model="picking_type_category">
    <field name="name">Equipment Out</field>
    <field name="code">EQO</field>
    <field name="direction">outgoing</field>
    <field name="default_destination_location_type_id" ref="location_type_equipment" />
    <field
                name="default_source_location_type_id"
                ref="ssi_stock.location_type_main_stock"
            />
    <field
                name="allowed_destination_location_type_ids"
                eval="[(6, 0, [ref('location_type_equipment')])]"
            />
    <field
                name="allowed_source_location_type_ids"
                eval="[(6, 0, [ref('ssi_stock.location_type_main_stock')])]"
            />
</record>
</data>
</odoo>
