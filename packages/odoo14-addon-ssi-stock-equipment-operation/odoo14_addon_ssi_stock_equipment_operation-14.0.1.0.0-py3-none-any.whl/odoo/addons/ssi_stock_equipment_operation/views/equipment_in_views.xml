<odoo>
<record id="equipment_in_action" model="ir.actions.act_window">
    <field name="name">Equipment In</field>
    <field name="type">ir.actions.act_window</field>
    <field name="res_model">stock.picking</field>
    <field name="view_mode">tree,form</field>
    <field
            name="domain"
            eval="[('picking_type_category_id','=',ref('picking_category_eqi'))]"
        />
    <field
            name="context"
            eval="{'default_picking_type_category_id':ref('picking_category_eqi')}"
        />
</record>

<menuitem
        id="equipment_in_menu"
        name="Equipment In"
        parent="ssi_stock.menu_incoming_movement"
        groups="equipment_in_viewer_group"
        action="equipment_in_action"
        sequence="105"
    />
</odoo>
